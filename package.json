{
  "name": "pixelwars-ui",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "anvil": "source .env && anvil -m \"$ANVIL_MNEMONIC\" --fork-url $ANVIL_FORK_URL",
    "build": "bun run wagmi && next build",
    "deploy:contract": "source .env && forge script contracts/script/Deploy.s.sol --rpc-url $FORGE_RPC_URL --broadcast --slow",
    "dev": "bun run wagmi && next dev",
    "dev:foundry": "bun run dev & wagmi generate --watch & bun run anvil",
    "start": "next start",
    "lint": "next lint",
    "wagmi": "wagmi generate"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/inter": "^5.0.8",
    "@mui/joy": "^5.0.0-beta.8",
    "@wagmi/cli": "^1.0.0",
    "next": "^13.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.3",
    "viem": "~0.3.36",
    "wagmi": "^1.0.6"
  },
  "devDependencies": {
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.15.0",
    "eslint-config-next": "^12.1.6",
    "postcss": "^8.4.31",
    "typescript": "^5.0.4"
  }
}